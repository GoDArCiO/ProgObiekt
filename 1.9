		// TODO Auto-generated method stub


        int[] tab=new int[5];
        int[] tab2=new int[5];
        int dzisiaj=2019;
        Scanner liczba = new Scanner(System.in);
        
        
        for(int i=0;i<tab.length;i++){
            System.out.println("Podaj nr pracownika");
